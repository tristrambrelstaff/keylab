<!DOCTYPE html>
<html>
  <head>
    <title>Keypad</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag "favicon.ico", rel: "icon", type: "image/vnd.microsoft.icon" %>
    <%= favicon_link_tag "apple-touch-icon.png", rel: "apple-touch-icon", type: "image/png"%>

    <%= stylesheet_link_tag "application" %>
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <div>
      <%= link_to image_tag("Keypad-logo.png"), root_path %>
      <%= link_to "Documents", documents_path %> | <%= link_to "Keys", keys_path %>
    </div>
    <div class="alert">
      <%= alert %>
    </div>
    <div class="notice">
      <%= notice %>
    </div>
    <%= yield %>
  </body>
</html>
